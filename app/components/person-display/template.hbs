<h1>{{this.changesetTree.changeset.name}}(changeset) vs {{this.changesetTree.model.name}}(model)
    vs {{this.changesetTree.changeset._content.name}}(changeset _content)</h1>
<h2>{{this.changesetTree.changeset.name}}'s Pets</h2>


<div {{did-insert this.didInsert}}>
</div>
{{!--{{tableData}}--}}
{{#if this.table}}
    <LightTable @table={{this.table}} @height='auto' as |t|>
        <t.head/>
        <t.body
            @canSelect={{false}}
            @onScrolledToBottom={{this.onScrolledToBottom}}
            as |body|>
            {{#if this.isLoading}}
                <body.loader>
                    <TableLoader/>
                </body.loader>
            {{/if}}
        </t.body>
        <t.foot fixed={{false}} as |columns|>
            <tr>
                <td class="align-center" colspan={{columns.length}}>
                    <ul class="pagination pagination-sm">
                        <li class={{if (eq this.currentPage 1) 'disabled'}}>
                            <a {{on 'click' (fn this.setPage (dec this.currentPage))}} href="#" aria-label="Previous">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>
                        {{#each (range 1 this.totalPages true) as |p|}}
                            <li class={{if (eq p this.currentPage) 'active'}}>
                                <a {{on 'click' (fn this.setPage p)}} href="#">{{p}}</a>
                            </li>
                        {{/each}}
                        <li class={{if (eq this.currentPage this.totalPages) 'disabled'}}>
                            <a {{on 'click' (fn this.setPage (inc this.currentPage))}} href="#" aria-label="Next">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    </ul>
                </td>
            </tr>
        </t.foot>
    </LightTable>
{{/if}}
<button onClick={{fn this.refresh}} type="button">Refresh</button>
{{yield}}